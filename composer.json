{
    "name": "femike/yii2-amqp-rrobin",
    "description": "Yii 2 extension wrapper to communicate with RabbitMQ server via AMQP. Based on videlalvaro/php-amqplib. forked webtoucher/yii2-amqp with supported round robin",
    "keywords": ["yii", "extension", "amqp", "RabbitMQ"],
    "homepage": "https://github.com/femike/yii2-amqp-rrobin",
    "type": "yii-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Alexey Kuznetsov",
            "email": "mirakuru@webtoucher.ru",
            "name": "Mikhail Samokhvalov",
            "email": "femike@mail.ru"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "php-amqplib/php-amqplib": "~2.0",
        "webtoucher/yii2-commands": "*"
    },
    "autoload": {
        "psr-4": {
            "webtoucher\\amqp\\": ""
        }
    }
}
